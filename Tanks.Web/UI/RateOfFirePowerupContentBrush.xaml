<UserControl x:Class="Tanks.Web.UI.RateOfFirePowerupContentBrush"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    Width="50" Height="50">
    <Canvas>
        <Canvas.Background>
            <SolidColorBrush x:Name="mainColor" Color="Transparent"/>
        </Canvas.Background>
        <StackPanel HorizontalAlignment="Center" Canvas.Left="7" Canvas.Top="1">
            <TextBlock HorizontalAlignment="Center" FontWeight="Bold" FontSize="17">+ +</TextBlock>
            <TextBlock HorizontalAlignment="Center" FontWeight="Bold" FontSize="17">+ +</TextBlock>
        </StackPanel>
    </Canvas>
</UserControl>
